    "next": "13.4.19",
    "next-i18next": "^14.0.0",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-query": "^3.39.3",
