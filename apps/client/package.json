    "@types/react-dom": "18.2.7",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.19",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "typescript": "5.1.6"
