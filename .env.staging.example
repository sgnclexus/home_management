# Staging Environment Configuration

# Application
NODE_ENV=staging
API_VERSION=v1
PORT=3001

# Firebase Configuration
FIREBASE_PROJECT_ID=your-staging-project-id
FIREBASE_CLIENT_EMAIL=your-staging-service-account@your-staging-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_STAGING_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
FIREBASE_DATABASE_URL=https://your-staging-project-id-default-rtdb.firebaseio.com/

# Payment Gateway Configuration (Test)
STRIPE_SECRET_KEY=sk_test_your_test_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_test_webhook_secret
PAYPAL_CLIENT_ID=your_test_paypal_client_id
PAYPAL_CLIENT_SECRET=your_test_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_test_paypal_webhook_id

# Security Configuration
JWT_SECRET=your_staging_jwt_secret
ENCRYPTION_KEY=your_staging_encryption_key
CORS_ORIGINS=https://your-staging-domain.vercel.app,http://localhost:3000
RATE_LIMIT_MAX=200
RATE_LIMIT_WINDOW_MS=900000

# Notification Services
FCM_SERVER_KEY=your_staging_fcm_server_key
VAPID_PRIVATE_KEY=your_staging_vapid_private_key

# Monitoring and Logging
SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
LOG_LEVEL=info

# Frontend Environment Variables (for Vercel)
NEXT_PUBLIC_FIREBASE_API_KEY=your_staging_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-staging-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-staging-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-staging-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_staging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_staging_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-YOUR_STAGING_MEASUREMENT_ID
NEXT_PUBLIC_API_BASE_URL=https://us-central1-your-staging-project-id.cloudfunctions.net/api
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_stripe_publishable_key
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_staging_vapid_public_key

# Deployment Configuration
VERCEL_TOKEN=your_vercel_token
VERCEL_ORG_ID=your_vercel_org_id
VERCEL_PROJECT_ID_STAGING=your_vercel_staging_project_id
FIREBASE_TOKEN=your_firebase_ci_token